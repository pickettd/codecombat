.container
  ol.skipped-contacts
    li.skipped-contact(v-for="skippedContact in skippedContacts")
      div(v-if="skippedContact")
        h2(v-if="skippedContact.trialRequest")
          {{ skippedContact.trialRequest.properties.email }}
        button.unarchive-contact(:data-contact-id="skippedContact._id", v-if="skippedContact.archived", @click="onClickUnarchiveContact")
          | Unarchive
        button.archive-contact(:data-contact-id="skippedContact._id", v-else, @click="onClickArchiveContact")
          | Archive

        div(v-if="!skippedContact.archived")
          h2
            | Reason skipped:
            {{ skippedContact.reasonSkipped }}
          pre
            {{ JSON.stringify(skippedContact, null, 4) }}
          pre(v-if="skippedContact.noteData")
            {{ skippedContact.noteData }}
